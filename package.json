  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/main.js",
    "deploy": "pm2 start dist/main.js --name art-pool-bl --watch",
    "dev": "nodemon --watch 'src/**/*.ts' --exec ts-node src/main.ts",
    "lint": "eslint 'src/**/*.ts'",
    "typeorm:migrate": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:run -d src/database/data.source.ts",
    "typeorm:generate": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:generate -d src/database/data.source.ts"
  },
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "bcrypt": "^6.0.0",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "http": "^0.0.1-security",
    "https": "^1.0.0",
    "inversify": "^7.8.1",
    "jsonwebtoken": "^9.0.2",
    "mqtt": "^5.14.0",
    "pg": "^8.16.3",
    "pm2": "^6.0.8",
    "reflect-metadata": "^0.2.2",
    "tsconfig-paths": "^4.2.0",
    "typeorm": "^0.3.26"
  },
  "devDependencies": {
    "@types/express": "^5.0.3",
    "@types/inversify": "^2.0.32",
    "@types/node": "^24.3.0",
    "eslint": "^9.33.0",
